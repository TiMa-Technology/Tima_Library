{
  "name": "@tima_technology/lib",
  "version": "1.3.4",
  "description": "Javascript utils for tima only.",
  "main": "dist/main.js",
  "module": "./dist/main.mjs",
  "types": "dist/main.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "package.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "scripts": {
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "build": "tsdown",
    "clean": "rm -rf dist coverage",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "format": "prettier --write 'src/**/*.{js,ts}'",
    "check-format": "prettier --check 'src/**/*.{js,ts}'",
    "local-release": "changeset version && changeset publish",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TiMa-Technology/Tima_Library.git"
  },
  "keywords": [
    "tima",
    "utils",
    "date",
    "format",
    "browser"
  ],
  "author": "Tima Tech",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TiMa-Technology/Tima_Library/issues"
  },
  "homepage": "https://github.com/TiMa-Technology/Tima_Library#readme",
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@eslint/compat": "^1.2.9",
    "@eslint/js": "^9.26.0",
    "@types/node": "^24.9.1",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "jsdom": "^27.0.1",
    "prettier": "^3.6.2",
    "tsdown": "^0.15.9",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vitest": "^3.2.4"
  }
}
