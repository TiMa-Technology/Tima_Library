{
  "name": "@tima_technology/lib",
  "version": "1.0.0",
  "description": "Javascript utils for tima only.",
  "main": "dist/main.js",
  "module": "./dist/main.mjs", 
  "types": "dist/main.d.ts",
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "scripts": {
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "build": "tsup src/main.ts --dts --out-dir dist --format esm",
    "clean": "rm -rf dist coverage",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TiMa-Technology/Tima_Library.git"
  },
  "keywords": [
    "tima",
    "utils",
    "date",
    "format",
    "browser"
  ],
  "author": "Tima Tech",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TiMa-Technology/Tima_Library/issues"
  },
  "homepage": "https://github.com/TiMa-Technology/Tima_Library#readme",
  "devDependencies": {
    "@eslint/compat": "^1.2.9",
    "@eslint/js": "^9.26.0",
    "@types/node": "^22.15.29",
    "@vitest/coverage-v8": "^3.1.4",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0",
    "vitest": "^3.1.4"
  }
}
